                SingleAxis(Analog(
                    input: GamePadRightTrigger2,
                    inversion: Inverted,
                    sensitivity: Sensitivity(5.0),
                )),
            ],
            "Blush": [
                Continuous(Hold([Key(ShiftLeft)])),
            ],
            "Teleport": [
                Pulse(JustPressed([Key(Space)])),
